package ui;
import javax.swing.*;

import java.awt.*;
import java.awt.event.*;
import java.io.*;

public class Mode_decide extends JFrame{
	private int Mx = 800, My = 640;
	private JPanel contentPane;//创建面板
	private JButton btnToCountFrm;//创建按钮1
	private JButton btnToSQL;//创建按钮2

	public Mode_decide(){
		super("Welcome to use Cara3.0");
		this.setResizable(false);
		this.setSize(Mx, My);
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.contentPane=new JPanel();//初始化面板
		this.contentPane.setLayout(null);//设置布局NULL
		this.btnToCountFrm=new JButton("按钮1");//给按钮名字
		this.btnToCountFrm.setBounds(6,6,90,30);//设置按钮名字
		this.contentPane.add(btnToCountFrm);//加入面板中
		this.btnToSQL=new JButton("按钮2");
		this.contentPane.add(btnToSQL);
		this.btnToSQL.setBounds(138, 26, 90, 30);
		this.add(this.contentPane);
		this.setVisible(true);
	}
	
	public static void main(String[] args){
		Mode_decide example=new Mode_deside();

		System.out.println("Mode decide Frame");
	}
}
